<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-5level  %msg%n</pattern>
        </encoder>
    </appender>

    <!-- quiet, jetty -->
    <logger name="org.eclipse.jetty" level="WARN" />

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
